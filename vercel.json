{
    "version": 2,
    "name": "blog-server",
    "buildCommand":"npm i fs-extra",
    "builds": [
        {
            "src": "http-server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "http-server.js"
        }
    ]
}
